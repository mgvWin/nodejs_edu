{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [ "airbnb", "prettier" ],
  "plugins": [ "prettier" ],
  "rules": {
    "prettier/prettier": [ "error" ],
    "import/extensions": [ "never" ],
    "no-console": "error",
    "class-methods-use-this": "off",
    "no-return-await": "off"
  }
}
